{
  "name": "testinator",
  "version": "1.0.0",
  "description": "Lightweight AI agent for E2E testing using Playwright MCP - supports multiple LLM providers",
  "type": "module",
  "main": "dist/main.js",
  "bin": {
    "testinator": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
