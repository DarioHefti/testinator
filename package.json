    "@ai-sdk/azure": "^1.1.0",
    "@ai-sdk/google": "^1.1.0",
    "@playwright/mcp": "^0.0.19",
    "playwright": "^1.52.0",
    "dotenv": "^16.4.7",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
